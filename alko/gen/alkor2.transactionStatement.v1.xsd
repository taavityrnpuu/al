<pseudoroot xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xrd="http://x-road.eu/xsd/xroad.xsd">

<!-- MOKAQUERY (transactionStatement) PARING -->
	<xsd:element name="transactionStatement">
	   <xsd:complexType>
            <xsd:sequence>
               <xsd:element name="Moka" type="tns:MokaType" />
            </xsd:sequence>
         </xsd:complexType>
	</xsd:element>
	
	
		<xsd:complexType name="MokaType">
            <xsd:annotation>
               <xsd:documentation>
                  Asutustele kontoliikumistest teavitamise formaat
               </xsd:documentation>
            </xsd:annotation>
            <xsd:all>
               <xsd:element name="Treasury" type="tns:Treasury" />
               <xsd:element name="Payer" type="tns:Payer">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>Maksja info</xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Receiver" type="tns:Receiver">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>Saaja info</xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Payment" type="tns:Payment" />
               <xsd:element name="TreasuryClassif" minOccurs="0">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Eelarveklassifikaator, mis näitab, millise
                           eelarvereaga on tehing seotud
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="100" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="PaymentDesc" minOccurs="0">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Tehinguinfo / maksjapoolne selgitus.
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="255" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:all>
         </xsd:complexType>
	
	
	
		<xsd:complexType name="Treasury">
            <xsd:all>
               <xsd:element name="DocumentNum">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>Dokumendi number.</xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="50" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="OrderNum">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Tehingu Riigikassapoolne unikaalne töökood
                           (arhiveerimistunnus).
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="1" />
                        <xsd:maxLength value="20" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="BankRef">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Tehingu pangapoolne arhiveerimistunnus juhul,
                           kui on sooritatud panga vahendusel.
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="20" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:all>
         </xsd:complexType>
	
		 
	
         <xsd:complexType name="Payer">
            <xsd:all>
               <xsd:element name="Name">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>Maksja nimi.</xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="80" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="RegNum">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Maksja id.-kood (isikukood, asutuse/ettevõtte
                           reg. nr.).
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="15" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="AccountNum" minOccurs="0">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Maksja arvelduskonto number.
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="35" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="RefNum" minOccurs="0">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Maksja viitenumber, mille alusel saab tehingu
                           siduda teise infosysteemiga.
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="20" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="TransmissionDate" minOccurs="0">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Tehingu Maksjapoolne väärtuspäev.
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:length value="10" />
                        <xsd:pattern value="\d{2}\.\d{2}\.\d{4}" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:all>
         </xsd:complexType>
	
		 <xsd:complexType name="Receiver">
            <xsd:all>
               <xsd:element name="Name" minOccurs="0">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>Saaja nimi.</xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="80" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="RegNum" minOccurs="0">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Saaja id.-kood (isikukood, asutuse/ettevõtte
                           reg. nr.).
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="15" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="AccountNum" minOccurs="0">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Saaja arvelduskonto number.
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="35" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="RefNum" minOccurs="0">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>Saaja viitenumber.</xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:minLength value="0" />
                        <xsd:maxLength value="20" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="ReceptionDate" minOccurs="0">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Tehingu teostamise kuupäev.
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:length value="10" />
                        <xsd:pattern value="\d{2}\.\d{2}\.\d{4}" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:all>
         </xsd:complexType>


	
        <xsd:complexType name="Payment">
            <xsd:all>
               <xsd:element name="Type" type="xsd:string">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Tehingu tüüp C või D näitab, kas tegemist on
                           kreedit- või deebetkandega konto omaniku
                           (asutuse) poolt vaadatuna.
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="BaseCurrencyAmount">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>Summa baasvaluutas.</xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:decimal">
                        <xsd:totalDigits value="15" />
                        <xsd:fractionDigits value="2" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="Amount">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>Summa valuutas.</xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:decimal">
                        <xsd:totalDigits value="15" />
                        <xsd:fractionDigits value="2" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
               <xsd:element name="Currency">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>
                           Valuuta kood ISO 4217 formaadis.
                        </xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
                  <xsd:simpleType>
                     <xsd:restriction base="xsd:string">
                        <xsd:pattern value="[a-zA-Z]{3}" />
                     </xsd:restriction>
                  </xsd:simpleType>
               </xsd:element>
            </xsd:all>
         </xsd:complexType>

	<!-- MOKAQUERY (transactionStatement) PARING LOPP -->
	
	<!-- MOKAQUERY (transactionStatement) PARINGU VASTUS -->
	<xsd:element name="transactionStatementResponse">
	    <xsd:complexType>
            <xsd:all>
               <xsd:element name="Message" type="xsd:string"
                  nillable="true">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>Teade</xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
               <xsd:element name="Code" type="xsd:int">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <xrd:title>kood</xrd:title>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
            </xsd:all>
         </xsd:complexType>
	</xsd:element>
	<!-- MOKAQUERY (transactionStatement) PARINGU VASUTS LOPP -->

</pseudoroot>