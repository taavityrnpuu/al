<pseudoroot xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xrd="http://x-road.eu/xsd/xroad.xsd">
	<xsd:element name ="Fault">
		<xsd:complexType>
			<xsd:annotation>
				<xsd:documentation>Fault reporting structure</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence>
				<xsd:element name="faultcode" type="xsd:QName"/>
				<xsd:element name="faultstring" type="xsd:string"/>
				<xsd:element name="faultactor" type="xsd:anyURI" minOccurs="0"/>
				<xsd:element name="detail" type="tns:detail" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="detail">
		<xsd:sequence>
			<xsd:any namespace="##any" minOccurs="0" maxOccurs="unbounded" processContents="lax"/>
		</xsd:sequence>
		<xsd:anyAttribute namespace="##any" processContents="lax"/>
	</xsd:complexType>
</pseudoroot>