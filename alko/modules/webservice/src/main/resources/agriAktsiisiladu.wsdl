        </wsdl:operation>
    </wsdl:portType>

    <!-- == BINDINGS == -->
    <wsdl:binding name="emtabind" type="tns:emtaport">
        <soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="agriAktsiisiladu">
            <xtee:version>v1</xtee:version>
            <soap:operation soapAction="http://producers.emta.xtee.riik.ee/producer/emta/agriAktsiisiladu"/>
            <wsdl:input>
                <soap:header message="tns:standardpais" part="asutus" use="encoded"
